    "version": "0.2.0",
    "configurations": [
        {
          "name": "local web",
          "request": "launch",
          "type": "dart",
          "program": "lib/main.dart",
          "args": [
            "-d",
            "chrome",
            "--web-port=4900",
            "--web-browser-flag",
            "--disable-web-security",
            "--flavor",
            "development",
            "--target",
            "lib/main.dart",
            "config.json"
          ]
        },
        {
        "name": "dev",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "args": [
          "--flavor",
          "development",
          "lib/main.dart",
          "config.json"
        ]
      },
    ]
